<%= form_for @network, url: {action: 'create'} do |f| %>
  
  <%= f.hidden_field(:admin_id, :value => @adminid) %>

  <b><%= f.label(:network_name, "Network Name: ") %></b>
  <%= f.text_field(:network_name, class: "net-name") %><br/>


  <%= f.submit("Create New Network", class: "new net-btn") %>

<% end %>



